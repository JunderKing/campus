view.comment_area{
  padding: 10px;
  background-color: #fff;
  margin: 10px 0;
}
view.comment_title{
  font-size: 14px;
  line-height: 22px;
  position: relative;
}
view.comment_title>button{
  position: absolute;
  width: 50px;
  height: 25px;
  line-height: 25px;
  text-align:center;
  padding: 0;
  right: 0;
  top: 0px;
}
view.comment_box{
  min-height: 55px;
  margin-bottom: 10px;
  position: relative;
  border-top: 1px solid #f0f0f0;
  color: #535353;
}
view.comment_box>image{
  width: 45px;
  height: 45px;
  border-radius: 5px;
  margin: 5px;
}
view.comment_box>view.content_box{
  margin: -55px 0 0 55px;
  font-size: 14px;
  line-height: 15px;
  position: relative;
}
view.content_box>text.nick_name{
  font-size: 16px;
  line-height: 20px;
  display: block;
  padding: 2px;
  color: rgba(96, 99, 152, 1);
}
view.score-bar{
  background-color: #bfbfbf;
  position: absolute;
  right: 5px;
  top: 2px;
  width: 90px;
  height: 20px;
}
view.score-bar>view.score-red{
  background-color: #e64340;
  height: 20px;
  position: absolute;
  left: 0;
  top: 0;
}
view.score-bar view.score-star-box{
  height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
