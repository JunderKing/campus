/*#####################################################################*/
/*
 * start-page
 */
view.start-page{
    width: 100%;
    height: 100vh;
    text-align: center;
    font-size: 25px;
    line-height: 100vh;
}

/*#####################################################################*/
/*###################################Project###########################*/
/*#####################################################################*/
/*
 * half-box
 */
.half-box {
    margin: 10px;
    border-radius: 5px;
    background-color: #fff;
    width: calc(100% - 20px);
    height: calc(50vh - 15px);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.half-box>text.text {
    font-size: 14px;
    color: #bfbfbf;
    display: block;
}
image.noproj-icon {
    width: 70px;
    height: 70px;
}


/*#####################################################################*/
/*###################################Camp##########################*/
/*#####################################################################*/
/*
 * *********************************************logo-area
 */
view.logo-area {
    padding-bottom: 5px;
    text-align: center;
    background-color: #fff;
    margin-top: -10px;
}
image.logo {
    display: inline-block;
    width: 60px;
    height: 60px;
}
/*
 * *****************************************************projListItem
 */
.proj-list-item {
    width: 100%;
    border-bottom: 1px solid #dcdcdc;
}
view.info-main {
    width: 100%;
    height: 60px;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
view.info-main>view {
    display: inline-block;
}
view.info-main>view.info-logo {
    width: 60px;
    height: 60px;
    padding: 7px;
}
view.info-logo>image {
    width: 46px;
    height: 46px;
    border-radius: 5px;
}
view.info-main>view.info-text {
    width: calc(100% - 120px);
    padding: 10px 0;
    font-size: 16px;
    line-height: 18px;
    color: #353535;
}
view.info-text>text.title {
    display: block;
    font-size: 18px;
    line-height: 22px;
    color: #888;
}
view.info-main>view.info-prog {
    width: 60px;
    padding: 9px 0 5px;
    font-size: 14px;
    line-height: 26px;
    color: #888;
    text-align: center;
}
view.info-prog>text.score {
    font-size: 16px;
    line-height: 20px;
}
view.info-extra {
    width: 100%;
    height: 20px;
    position: relative;
}
view.info-extra>view.tag-box {
    height: 20px;
    padding: 2px 5px;
    max-width: 60%;
    overflow: hidden;
    display: flex;
    /*justify-content: space-between;*/
}
view.tag-box>text {
    box-sizing: border-box;
    display: inline-block;
    height: 16px;
    padding: 0px 2px;
    margin: 0 2px;
    border-radius: 3px;
    font-size: 10px;
    line-height: 16px;
    color: #f0f0f0;
    background-color: #E46C53;
}
view.info-extra>view.score-bar {
    width: 70px;
    height: 16px;
    top: 0;
}
view.score-bar.project>view.score-red {
    height: 16px;
}
/*
 * ******************************************************proj-list
 */
view.proj-list.content-area{
    padding-bottom: 0;
    padding-top: 0;
}
view.proj-info{
    padding: 10px 0;
    border-top: 1px solid #dcdcdc;
    position: relative;
}
view.proj-info>image{
    width: 45px;
    height: 45px;
    border-radius: 5px;
}
view.proj-info>text.nick-name{
    position: absolute;
    left: 55px;
    top: 14px;
    font-size: 16px;
    color: #888;
}
view.proj-info>text.proj-title{
    position: absolute;
    left: 55px;
    top: 34px;
    font-size: 16px;
    color: #353535;
    width: calc(100% - 55px);
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
view.proj-info>view.del-btn {
    position: absolute;
    right: 5px;
    top: 15px;
}
/* proj-choose*/
view.proj-list.choose {
    background-color: #fff;
    width: 100%;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #f0f0f0;
    padding: 0 10px;
}

