/*
 * project
 */
view.menu-box{
  display: inline-flex;
  width: 100%;
  height: 100px;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-around;
  align-items: center;
}
view.menu-box .menu-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 25vw;
  height: 25vw;
  /*border: 1px solid #888;*/
  /*border-radius: 10px;*/
  font-size: 14px;
  line-height: 20px;
  text-align: center;
  color: #888;
}
.menu-item image{
  display: block;
  width: 10vw;
  height: 10vw;
}
view.image-button {
  width: 18vw;
  height: 18vw;
  border-radius: 9vw;
  display: flex;
  justify-content: center;
  align-items: center;
}
view.image-button.report{
  background-color: #3AB4A6;
}
view.image-button.card{
  background-color: #25A365;
}
view.image-button.canvas{
  background-color: #99C53E;
}
/*
 * proj-list
 */
view.proj-list.content-area{
  padding-bottom: 0;
  padding-top: 0;
}
view.proj-info{
  padding: 10px 0;
  border-top: 1px solid #dcdcdc;
  position: relative;
}
view.proj-info>image{
  width: 45px;
  height: 45px;
  border-radius: 5px;
}
view.proj-info>text.nick-name{
  position: absolute;
  left: 55px;
  top: 14px;
  font-size: 16px;
  color: #888;
}
view.proj-info>text.proj-title{
  position: absolute;
  left: 55px;
  top: 34px;
  font-size: 16px;
  color: #353535;
  width: calc(100% - 55px);
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
/*
 * info-area
 */
view{
  box-sizing: border-box;
}
view.info-area {
  width: 100%;
  background-color: #E28428;
  padding-bottom: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
}
image.avatar.info {
  width: 80px;
  height: 80px;
  border-radius: 40px;
}
view.info-area>view.info-box {
  width: calc(70% - 80px);
  height: 70px;
  display: flex;
  padding-left: 10px;
  flex-direction: column;
  justify-content: space-around;
  font-size: 16px;
  color: #fff;
}
view.info-box>text.info-name {
  font-size: 20px;
}
