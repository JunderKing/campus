view.interval-title {
  box-sizing: border-box;
  padding: 5px;
  width: 100%;
  font-size: 16px;
  color: #fff;
  background-color: #E28428;
}

/*app-list*/
view.app-list {
  margin-bottom: 10px;
  width: 100%;
}
view.app-box {
  display: inline-block;
  text-align: center;
  width: 33%;
  font-size: 16px;
  line-height: 20px;
  color: #353535;
  position: relative;
}
view.app-box image{
  display: block;
  width: 40px;
  height: 40px;
  margin: 0 calc(50% - 20px);
}
text.app-status {
  position: absolute;
  right: 5px;
  top: 2px;
  font-size: 10px;
  line-height: 12px;
  height: 12px;
  background-color: #E60012;
  border-radius: 2px;
  padding: 2px;
  color: #fff;
}
text.app-status.gray {
  background-color: #b2b2b2;
}
/*growth-line*/
view.growth-line {
  box-sizing: border-box;
  width: 100%;
  padding: 5px 2px 0;
  color: #888;
}
view.growth-cursor {
  margin-bottom: -15px;
}
view.growth-cursor image{
  height: 30px;
  width: 30px;
  transform: translate(45px);
}
view.growth-line view.dot {
  box-sizing: border-box;
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 4px;
  background-color: #888;
}
view.growth-line view.line {
  position: relative;
  bottom: 3px;
  box-sizing: border-box;
  display: inline-block;
  width: calc(16.6% - 6.66px);
  height: 2px;
  background-color: #888;
}
view.growth-scale {
  margin-top: -8px;
  box-sizing: border-box;
  width: 100%;
  padding: 0 25px;
}
text.scale {
  display: inline-block;
  width: 20%;
  text-align: center;
  font-size: 10px;
}
view.growth-line view.green {
  background-color: #00913A;
}







/*puls-button-box*/
view.pulse-box>navigator{
  display: inline-block;
  height: 25vw;
  width: 25vw;
  margin: 0 10px;
  text-align: center;
  border: 1px solid #dcdcdc;
  border-radius: 5px;
  line-height: 25vw;
  font-size: 22px;
  position: relative;
}
/*test-card*/
view.test-card-box{
}
navigator.test-card, view.test-card{
  color: #fff;
  padding: 10px;
  height: 50px;
  position: relative;
  margin-bottom: 6px;
}
navigator.test-card>view.card-title, view.test-card>view.card-title{
  font-size: 18px;
  line-height: 40px;
}
navigator.test-card>view.card-footer, view.test-card>view.card-footer{
  font-size: 14px;
  line-height: 16px;
  color: #dcdcdc;
  position: absolute;
  bottom: 5px;
  left: 10px;
}
navigator.test-card.state0, view.test-card.active{
  background-color: rgba(14, 175, 255, 1);
}
navigator.test-card.state1, view.test-card.failed{
  background-color: rgba(247, 96, 96, 1);
}
navigator.test-card.state2, view.test-card.success{
  background-color: rgba(9, 187, 9, 1);
}



view.card-page-title{
  box-sizing: border-box;
  font-size: 22px;
  height: none;
  padding: 20px 5px 10px;
  line-height: 26px;
  color: #353535;
  background-color: #fff;
  text-align: center;
}

view.noproj-text{
  font-size: 14px;
  line-height: 16px;
  width: 100%;
  text-align: center;
  color: #b2b2b2;
  padding: 5px;
}
view.noproj-text text{
  display: block;
}


view.proj-list.choose {
  background-color: #fff;
  width: 100%;
  height: 50px;
  line-height: 50px;
  border-bottom: 1px solid #f0f0f0;
  padding: 0 10px;
}

.pulse-content{
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
navigator.record-item {
  position: relative;
}
text.record-date {
  display: block;
  position: absolute;
  bottom: 10px;
  right: 10px;
  /*left: 60px;*/
  color: #888;
  font-size: 12px;
}
.foot-text {
  color: #888;
  font-size: 14px;
}
view.border-box {
  border-bottom: 1px solid #f0f0f0;
}
text.avatar-nickName {
  width: 100%;
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  text-align: center;
}
