/*
 * menu-box
 */
view.menu-box{
    display: inline-flex;
    width: 100%;
    height: 100px;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-around;
    align-items: center;
}
view.menu-box>.menu-item{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 25vw;
    height: 25vw;
    /*border: 1px solid #888;*/
    /*border-radius: 12.5vw;*/
    font-size: 14px;
    line-height: 20px;
    text-align: center;
    color: #888;
}
.menu-item image{
    display: block;
    width: 10vw;
    height: 10vw;
}
view.image-button {
    width: 18vw;
    height: 18vw;
    border-radius: 9vw;
    display: flex;
    justify-content: center;
    align-items: center;
}
view.image-button.report{
    background-color: #FFBE0E;
}
view.image-button.canvas{
    background-color: #3DC4FC;
}
view.image-button.card{
    background-color: #E46C53;
}

/*test-card*/
view.test-card-box{
}
navigator.test-card, view.test-card{
    color: #fff;
    padding: 10px;
    height: 50px;
    position: relative;
    margin-bottom: 6px;
}
navigator.test-card>view.card-title, view.test-card>view.card-title{
    font-size: 18px;
    line-height: 40px;
}
navigator.test-card>view.card-footer, view.test-card>view.card-footer{
    font-size: 14px;
    line-height: 16px;
    color: #dcdcdc;
    position: absolute;
    bottom: 5px;
    left: 10px;
}
navigator.test-card.state0, view.test-card.active{
    background-color: rgba(14, 175, 255, 1);
}
navigator.test-card.state1, view.test-card.failed{
    background-color: rgba(247, 96, 96, 1);
}
navigator.test-card.state2, view.test-card.success{
    background-color: rgba(9, 187, 9, 1);
}




view.item-list{
    padding: 10px 10px 0;
}
navigator.item-box{
    padding: 10px 0;
    border-top: 1px solid #dcdcdc;
    font-size: 16px;
    position: relative;
}
navigator.item-box>text.content{
    position: absolute;
    left: 60px;
    top: 10px;
    font-size: 16px;
}
view.text-box{
    text-align: center;
    font-size: 30px;
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    width: 50px;
    height: 50px;
    line-height: 50px;
    color: #808080;
}
text.pulse-content{
    font-size: 14px;
    line-height: 16px;
    display: block;
    max-height: 32px;
    color: #353535;
    overflow:hidden; 
    text-overflow:ellipsis;
    display:-webkit-box; 
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2; 
}
navigator.record-item {
    position: relative;
}
text.record-date {
    display: block;
    position: absolute;
    bottom: 10px;
    right: 10px;
    color: #888;
    font-size: 12px;
}

.foot-text {
    color: #888;
    font-size: 14px;
}
view.border-box {
    border-bottom: 1px solid #f0f0f0;
}
