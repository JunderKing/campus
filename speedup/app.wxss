/*
 * #000 Black-主要
 * #353535 Semi-大段主要
 * #888 Gray-次要
 * #b2b2b2 Light-时间戳，缺省值
 * 蓝色#576b95, 20%, 10%
 * 绿色#09bb07
 * 红色#e64340
 */
/*#####################################################################*/
/*###################################Global############################*/
/*#####################################################################*/
button[type="primary"] {
    /*background-color: #E28428;*/
}
view {
    box-sizing: border-box;
}
.ellipsis{
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
/*
 *******************************************************main
 */
view.container {
    width: 100%;
    min-height: 100vh;
    padding-top: 1px;
    background-color: #f0f0f0;
}
scroll-view.container {
    width: 100%;
    height: 100vh;
    background-color: #f0f0f0;
}
view.container-white {
    width: 100%;
    min-height: 100vh;
    background-color: #fff;
}
view.container-white.qrcode {
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
view.container-white.qrcode image{
    display: block;
    position: relative;
    bottom: 30px;
    width: 200px;
    height: 200px;
}

view.page-title {
    width: 100%;
    padding: 20px;
    text-align: center;
    font-size: 20px;
    color: #000;
    background-color: #fff;
}
view.page-title>navigator{
    width: 100%;
    padding: 0 10px;
    box-sizing: border-box;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
view.interval-title {
    padding: 5px;
    width: 100%;
    font-size: 16px;
    color: #fff;
    background-color: #000;
}
view.main-hint {
    padding: 5px;
    width: 100%;
    font-size: 14px;
    color: #888;
    text-align: center;
}
view.page-footer {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    background-color: #fff;
    border-top: 1px solid #cdcdcd;
}
/*
 * content area
 */
.content-area{
    padding: 5px;
    background-color: #fff;
    min-height: 50px;
}
.para {
    display: block;
    margin: 5px;
    font-size: 16px;
    line-height: 18px;
    color: #353535;
}
/*
 * button area
 */
view.btn-area{
    padding-bottom: 1px;
    padding: 10px 10px 1px;
}
view.btn-area button{
    margin-bottom: 9px;
}
picker.btn-style{
    margin-bottom: 9px;
    width: 100%;
    color: #000;
    background-color: #f8f8f8;
    font-size: 18px;
    line-height: 2.55;
    border: 1px solid #dcdcdc;
    border-radius: 5px;
    text-align: center;
    box-sizing: border-box;
}
picker.btn-style.primary {
    color: #fff;
    background-color: #1AAD19;
}
/*
 * **************************************************bottom-float
 */
view.bottom-anchor{
    width: 100%;
    height: 8vh;
}
view.bottom-float{
    width: 100%;
    height: 9vh;
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background-color: #fff;
    color: #bfbfbf;
    border-top: 1px solid #bfbfbf;
    font-size: 10px;
    text-align: center;
}
view.bottom-float text{
    display: block;
    line-height: 16px;
}
navigator.icon-box.float-btn{
    width: 65%;
}
navigator.icon-box.float-btn text{
    background-color: #09bb07;
    color: #fff;
    font-size: 14px;
    line-height: 6vh;
    border-radius: 5px;
}
/*
 * ******************************************************avatar area
 */
view.avatar-area {
    padding: 5px;
    text-align: left;
    background-color: #fff;
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex-wrap: wrap;
}
view.avatar-box {
    display: inline-block;
    width: 60px;
    text-align: center;
    font-size: 12px;
    line-height: 14px;
    color: #888;
    position: relative;
}
view.avatar-box>image.avatar {
    width: 46px;
    height: 46px;
    border-radius: 23px;
}
view.avatar-box>image.avatar.cube {
    border-radius: 3px;
}
view.avatar-box>text {
    width: 100%;
    display: block;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
view.del-icon {
    width: 14px;
    height: 14px;
    border-radius: 7px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: red;
    position: absolute;
    right: 4px;
    top: 0px;
}
view.del-icon>view{
    width: 8px;
    height: 2px;
    border-radius: 1px;
    background-color: #fff;
}
/*
 * ******************************************************form表单
 */
view.group-title {
    font-size: 14px;
    color: #888;
    padding: 10px 10px 3px;
}
view.form-group {
    background-color: #fff;
    padding: 0 10px;
}
view.form-group.small {
    display: inline-block;
    width: calc(100% - 80px);
    padding-left: 0;
}
view.form-item-logo {
    width: 80px;
    height: 70px;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    position: relative;
    top: 2px;
    border-left: 1px solid #f0f0f0;
}
view.form-item-logo>.item-title {
    position: absolute;
    top: 0px;
    width: 100%;
    text-align: center;
    font-size: 12px;
    color: #888;
}
view.form-item-logo>image {
    width: 55px;
    height: 55px;
    position: relative;
    top: 6px;
}
view.form-item {
    display: flex;
    font-size: 16px;
    line-height: 40px;
    min-height: 40px;
}
view.form-item.border {
    border-bottom: 1px solid #f0f0f0;
}
view.form-item.border-top {
    border-top: 1px solid #f0f0f0;
}
view.form-item>.item-title {
    min-width: 64px;
    font-weight: bold;
    height: 40px;
}
view.form-item>view.item-content {
    padding-left: 10px;
    flex: 1 1 auto;
    min-height: 40px;
}
/*view.form-item>view.item-content.big {*/
/*height: none;*/
/*min-height: 40px;*/
/*}*/
view.item-content>input {
    width: 100%;
    height: 40px;
}
view.item-content>picker {
    width: 50%;
    height: 40px;
    display: inline-block;
}
view.item-content>textarea {
    width: 100%;
    padding: 12px 0;
    min-height: 50px;
    font-size: 16px;
    line-height: 18px;
    /*padding-top: 5px;*/
}
view.item-content>radio-group {
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
view.item-content>image.score {
    width: 30px;
    height: 30px;
    margin: 5px 3px;
}
view.item-content>picker.big {
    width: 100%;
}
view.item-content>picker.def {
    color: #888;
}
view.form-item>textarea {
    width: 100%;
    min-height: 80px;
    font-size: 16px;
    line-height: 18px;
    padding-top: 5px;
}
view.form-item>input {
    width: 100%;
}

/*
 * ***************************************************comment-area
 */
view.comment-area{
    padding: 10px;
    background-color: #fff;
    margin: 10px 0;
}
view.comment-title{
    font-size: 14px;
    line-height: 22px;
    position: relative;
}
view.comment-box{
    min-height: 55px;
    margin-bottom: 10px;
    position: relative;
    border-top: 1px solid #f0f0f0;
    color: #535353;
}
view.comment-box>image{
    width: 45px;
    height: 45px;
    border-radius: 5px;
    margin: 5px;
}
view.comment-box>view.content-box{
    margin: -55px 0 0 55px;
    font-size: 14px;
    line-height: 15px;
    position: relative;
}
view.content-box>text.nick-name{
    font-size: 16px;
    line-height: 20px;
    display: block;
    padding: 2px;
    color: #606398;
}

view.score-bar{
    background-color: #bfbfbf;
    position: absolute;
    right: 5px;
    top: 2px;
    width: 90px;
    height: 20px;
}
view.score-bar>view.score-red{
    background-color: #e64340;
    height: 20px;
    position: absolute;
    left: 0;
    top: 0;
}
view.score-bar view.score-star-box{
    height: 100%;
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

