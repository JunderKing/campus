/*
 * OverAll
 */
button[type="primary"] {
  background-color: #E28428;
}
view.container {
  width: 100vw;
  min-height: 100vh;
  background-color: #f0f0f0;
  overflow: hidden;
}
scroll-view.container {
  width: 100vw;
  height: 100vh;
  background-color: #f0f0f0;
}
view.container-white {
  width: 100%;
  min-height: 100vh;
  background-color: #fff;
}
.half-box {
  margin: 10px;
  border-radius: 5px;
  background-color: #fff;
  /*border: 1px solid #bfbfbf;*/
  width: calc(100% - 20px);
  height: calc(50vh - 15px);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.half-box>text.text {
  font-size: 14px;
  color: #bfbfbf;
  display: block;
}
image.noproj-icon {
  width: 70px;
  height: 70px;
}
view.page-title {
  width: 100%;
  height: 50px;
  text-align: center;
  font-size: 20px;
  color: #fff;
  background-color: #E28428;
  text-align: center;
}


view.page-title-area {
  width: 100%;
  background-color: #E28428;
  padding-bottom: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
}
image.title-logo {
  width: 70px;
  height: 70px;
  border-radius: 35px;
}
view.page-title-area>view.title-info-box {
  min-width: 40%;
  max-width: calc(80% - 70px);
  height: 70px;
  display: flex;
  padding-left: 10px;
  flex-direction: column;
  justify-content: space-around;
  font-size: 14px;
  color: #fff;
}
view.title-info-box>text.info-name {
  font-size: 16px;
  line-height: 18px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  /*overflow:hidden; */
  /*text-overflow:ellipsis;*/
  /*display:-webkit-box; */
  /*-webkit-box-orient:vertical;*/
  /*-webkit-line-clamp:2; */
}


view.page-title navigator{
  width: 100%;
  padding: 0 10px;
  box-sizing: border-box;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
view.page-footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  background-color: #fff;
  border-top: 1px solid #cdcdcd;
}
.content-area{
  padding: 10px;
  background-color: #fff;
}
view.content-area.list-box {
  padding: 0 10px;
}
view.content-area.time-box text{
  display: block;
  line-height: 24px;
}
.para {
  display: block;
  font-size: 16px;
  line-height: 18px;
  min-height: 50px;
  color: #353535;
}
view.btn-area{
  margin-top: 10px;
  padding-bottom: 1px;
}
view.btn-area button{
  margin: 10px;
}
picker.btn-style{
  margin: 10px;
  width: calc(100% - 20px);
  color: #000;
  background-color: #f8f8f8;
  font-size: 18px;
  line-height: 2.55;
  border: 1px solid #dcdcdc;
  border-radius: 5px;
  text-align: center;
  box-sizing: border-box;
}
navigator.nav-btn{
  width: calc(100% - 20px); 
  text-align: center;
  font-size: 18px;
  line-height: 50px;
  border: 1px solid #dcdcdc;
  color: #888;
  margin: 0 10px;
}
/*
 * avatar area
 */
view.avatar-area {
  padding: 5px;
  /*margin-top: -10px;*/
  text-align: left;
  background-color: #fff;
}
view.sponsor-area {
  padding-bottom: 5px;
  text-align: center;
  background-color: #fff;
  margin-top: -8px;
}
view.avatar-box {
  display: inline-block;
  margin: 0 5px;
  width: 50px;
  text-align: center;
  font-size: 12px;
  line-height: 14px;
  position: relative;
  color: #888;
}
view.avatar-box image.tiny-del {
  position: absolute;
  top: -4px;
  left: -4px;
  width: 20px;
  height: 20px;
}
image.avatar {
  display: block;
  width: 46px;
  height: 46px;
  border-radius: 23px;
}
image.avatar.icon {
  border-radius: 3px;
}
image.logo {
  display: block;
  width: 60px;
  height: 60px;
}
/*
 * comment-area
 */
view.comment-area{
  padding: 10px;
  background-color: #fff;
  margin: 10px 0;
}
view.comment-title{
  font-size: 14px;
  line-height: 22px;
  position: relative;
}
view.comment-title>button{
  position: absolute;
  width: 50px;
  height: 25px;
  line-height: 25px;
  text-align:center;
  padding: 0;
  right: 0;
  top: 0px;
}
view.comment-box{
  min-height: 55px;
  margin-bottom: 10px;
  position: relative;
  border-top: 1px solid #f0f0f0;
  color: #535353;
}
view.comment-box>image{
  width: 45px;
  height: 45px;
  border-radius: 5px;
  margin: 5px;
}
view.comment-box>view.content-box{
  margin: -55px 0 0 55px;
  font-size: 14px;
  line-height: 15px;
  position: relative;
}
view.content-box>text.nick-name{
  font-size: 16px;
  line-height: 20px;
  display: block;
  padding: 2px;
  color: rgba(96, 99, 152, 1);
}
view.comment-btn-box{
  margin: 3px;
  font-size: 12px;
  line-height: 15px;
  position: relative;
  color: rgba(164,164,164,1);
}
view.comment-btn-box>navigator.reply-btn{
  display: block;
  position: absolute;
  font-weight: bold;
  font-size: 14px;
  top: 0;
  right: 5px;
  color: rgba(131,147,180,1);
}
view.reply-area{
  margin: 5px 0 0;
}
view.reply-box{
  background-color: rgba(238, 238, 238, 0.5);
  padding:3px;
}
view.reply-box>text.nick-name{
  color: rgba(96, 99, 152, 1);
}
/*
 * bottom-float
 */
view.bottom-anchor{
  width: 100%;
  height: 8vh;
}
view.bottom-float{
  width: 100%;
  height: 9vh;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
  color: #bfbfbf;
  border-top: 1px solid #bfbfbf;
  font-size: 10px;
  text-align: center;
}
view.bottom-float text{
  display: block;
  line-height: 16px;
}
navigator.icon-box.float-btn{
  width: 70%;
}
navigator.icon-box.float-btn text{
  background-color: #09bb07;
  color: #fff;
  font-size: 14px;
  line-height: 6vh;
  border-radius: 5px;
}
/*
 * form表单
 */
view.area-title{
  font-size: 14px;
  color: #888;
  line-height: 16px;
  padding: 10px 5px 5px;
}
view.form-area {
  background-color: #fff;
  padding: 5px;
  display: flex;
  /*flex-flow: row nowrap;*/
  justify-content: space-around;
  align-items: center;
  font-size: 16px;
  color: #353535;
}
view.form-area.column {
  flex-direction: column;
}
view.form-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  min-height: 40px;
}
view.form-item.border {
  border-bottom: 2px solid #f0f0f0;
}
view.form-item.cube {
  width: 80px;
  height: 80px;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 2px solid #f0f0f0;
}
view.form-item.cube>image.logo {
  position: relative;
  top: 5px;
}
view.form-item-box {
  width: calc(100% - 85px)
}
view.item-title {
  font-size: 16px;
  font-weight: bold;
  text-align: center;
}
view.item-title-float {
  position: absolute;
  width: 100%;
  top: 0px;
  text-align: center;
  font-size: 10px;
  font-weight: bold;
  color: #888;
}
view.form-area>input {
  width: 100%;
}
view.form-area>textarea {
  width: 100%;
  min-height: 100px;
}
picker.picker-default {
  color: #888;
}
view.form-item {
  display: relative;
  width: 100%;
}
view.form-item>view.item-title {
  width: 20%;
  display: inline-block;
}
view.form-item.big>view.item-title {
  width: 90px;
}
view.form-item>view.item-content {
  width: 80%;
  display: inline-block;
}
view.form-item.big>view.item-content {
  width: calc(100% - 90px)
}
view.item-content>input {
  width: 100%;
}
view.item-content>textarea {
  /*min-height: 100px;*/
  width: 100%;
  padding: 3px 0;
}
