view.comment-btn-box{
    margin: 3px;
    font-size: 12px;
    line-height: 15px;
    position: relative;
    color: #A4A4A4;
}
view.comment-btn-box>navigator.reply-btn{
    display: block;
    position: absolute;
    font-weight: bold;
    font-size: 14px;
    top: 0;
    right: 5px;
    color:#8393B4;
}
view.reply-area{
    margin: 5px 0 0;
}
view.reply-box{
    background-color: rgba(238, 238, 238, 0.5);
    padding:3px;
}
view.reply-box>text.nick-name{
    color: #606398;
}
