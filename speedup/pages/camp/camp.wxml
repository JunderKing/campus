<view class="container">
  <view class="" wx:if="{{campId}}">
  <view class="page-title-area">
    <image class="title-logo" mode="aspectFill" src="{{logoUrl}}"></image>
    <view class="title-info-box">
      <text class="info-name">{{name}}</text>
      <text class="info-province">主办方：{{sponsor}}</text>
      <!--<text class="info-tag">{{tag}}</text>-->
    </view>
  </view>
    <!--<view class="sponsor-area">-->
      <!--<view class="avatar-box" wx:for="{{logos}}"> -->
        <!--<image class="logo" src="{{item}}" mode="aspectFit"></image>-->
        <!--</view>-->
      <!--</view>-->
    <!--<view class="interval-title">-->
      <!--简介-->
      <!--</view>-->
    <view class="para content-area">
      {{intro}}
    </view>
    <view class="interval-title">
      项目列表
    </view>
    <view class="noproj-text" wx:if="{{!projList[0]}}">
      本次加速营还没有项目哦
    </view>
    <view class="proj-list content-area">
      <navigator url="/pages/camp/projDetail?projId={{item.projId}}" wx:for="{{projList}}" hover-class="navigator-hover">
        <view class="proj-info">
          <image mode="scaleToFill" src="{{item.logoUrl}}"></image>
          <text class="nick-name">{{item.nickName}}</text>
          <text class="proj-title">{{item.name}}</text>
        </view>
      </navigator>
    </view>
  </view>
  <view class="noproj-text" wx:else >
    还没有创建加速营
  </view>
  <view class="btn-area">
    <navigator url="/pages/camp/campAdd" hover-class="navigator-hover">
      <button type="primary" size="default" wx:if="{{true}}">创建加速营</button>
    </navigator>
    <navigator url="/pages/camp/campDetail?campId={{campId}}" hover-class="navigator-hover">
      <button type="default" size="default" wx:if="{{true}}">管理加速营</button>
    </navigator>
    <picker class="btn-style" mode="selector" range="{{campList}}" range-key="name" bindchange="onCampChange">切换加速营</picker>
  </view>
</view>
