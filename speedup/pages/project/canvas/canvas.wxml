        <text class="card-num" wx:if="{{cardNum[1]}}">{{cardNum[1]}}</text>
      </navigator>
    </view>
    <view class="canvas-row">
      <navigator class="canvas-item detail-canvas" url="/pages/project/canvas/gridDetail?projId={{projId}}&gridNum=3&isUser={{isUser}}">
