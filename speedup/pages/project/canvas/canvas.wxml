        <text class="card-num" wx:if="{{cardNum[3]}}">{{cardNum[3]}}</text>
      </navigator>
    </view>
    <view class="canvas-row">
      <navigator class="canvas-item detail-canvas" url="/pages/project/canvas/gridDetail?projId={{projId}}&gridNum=5&isUser={{isUser}}">
        <view class="marker" wx:if="{{marker.indicator}}"></view>
        <text>⑤将如何</text>
        <text class="card-num" wx:if="{{cardNum[4]}}">{{cardNum[4]}}</text>
      </navigator>
      <view class="canvas-item title">
        <text>未来</text>
      </view>
      <navigator class="canvas-item painpoiont-canvas" url="/pages/project/canvas/gridDetail?projId={{projId}}&gridNum=6&isUser={{isUser}}">
