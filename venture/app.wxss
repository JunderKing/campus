/*
 * #000 Black-主要
 * #353535 Semi-大段主要
 * #888 Gray-次要
 * #b2b2b2 Light-时间戳，缺省值
 * 蓝色#576b95, 20%, 10%
 * 绿色#09bb07
 * 红色#e64340
 */
/*#####################################################################*/
/*###################################Global############################*/
/*#####################################################################*/
button[type="primary"] {
  /*background-color: #E28428;*/
}
view {
  box-sizing: border-box;
}
.ellipsis{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;  
}
/*
 *******************************************************main
 */
view.container {
  width: 100%;
  min-height: 100vh;
  padding-top: 1px;
  background-color: #f0f0f0;
}
scroll-view.container {
  width: 100%;
  height: 100vh;
  background-color: #f0f0f0;
}
view.container-white {
  width: 100%;
  min-height: 100vh;
  background-color: #fff;
}

view.page-title {
  width: 100%;
  padding: 20px;
  text-align: center;
  font-size: 20px;
  color: #000;
  background-color: #fff;
}
view.page-title>navigator{
  width: 100%;
  padding: 0 10px;
  box-sizing: border-box;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
view.interval-title {
  padding: 5px;
  width: 100%;
  font-size: 16px;
  color: #fff;
  background-color: #000;
}
view.main-hint {
  padding: 5px;
  width: 100%;
